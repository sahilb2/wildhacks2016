{
    "manifest_version": 2,
    "name": "NSFWBlock",
    "version": "0.1",
    "description": "Using Clarifai to block NSFW images / websites",
    "browser_action": {
    "name": "",
    "icons": ["aniconhere"],
    "default_icon": "aniconere",
    "default_popup": "popup.html",
    "default_title": "NSFW images blocker"
  },
  "content_scripts": [ {
    "css": ["basic.css"],
    "js": [ "jquery-1.12.0.min.js", "contentscript.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
